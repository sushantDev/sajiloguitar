<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.sajiloguitar" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Sajilo Guitar</name>
    <description>
        Sajilo Guitar is collection of Nepali songs lyrics and cords. Useful for all level guitar players and everyone seeking Nepali lyrics.
    </description>
    <author email="sushantsapkota07@gmail.com" href="http://sushantsapkota.cf/">
        Sushant Sapkota
    </author>
    <content src="index.html" />
    <splash src="www/res/screen/android/splash.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
